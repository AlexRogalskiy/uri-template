{
    "vars": {
        "var": "value",
        "hello": "Hello World!",
        "empty": ""
    },
    "tests": {
        "{var}": "value",
        "{+var}": "value",
        "{hello}": "Hello%20World%21",
        "{+hello}": "Hello%20World!",
        "X{undef}": "X",
        "{empty}": "",
        "{+empty}": "",
        "{.empty}": ".",
        "{/empty}": "/",
        "{;empty}": ";empty",
        "{?empty}": "?empty=",
        "{&empty}": "&empty=",
        "{#empty}": "#"
    }
}